rnal":false,"editable":false,"type":"Activities::ReportBecamePublic","message":"","markdown_message":"","automated_response":false,"created_at":"2020-08-21T20:20:16.690Z","updated_at":"2020-08-21T20:20:16.690Z","actor":{"username":"aaronloo","cleared":false,"url":"/aaronloo","profile_picture_urls":{"medium":"/assets/avatars/default-71a302d706457f3d3a31eb30fa3e73e6cf0b1d677b8fa218eaeaffd67ae97918.png"},"hackerone_triager":false,"hackerone_employee":false},"genius_execution_id":null,"team_handle":"yelp","actor_is_team_member":true,"actor_is_concealed_member":false},{"id":9005460,"is_internal":false,"editable":false,"type":"Activities::ReportSeverityUpdated","message":"","markdown_message":"","automated_response":false,"created_at":"2020-08-21T20:36:50.875Z","updated_at":"2020-08-21T20:36:50.875Z","additional_data":{"old_severity":"High","new_severity":"Low (3.5)","old_severity_id":192800,"new_severity_id":825380},"actor":{"username":"aaronloo","cleared":false,"url":"/aaronloo","profile_picture_urls":{"medium":"/assets/avatars/default-71a302d706457f3d3a31eb30fa3e73e6cf0b1d677b8fa218eaeaffd67ae97918.png"},"hackerone_triager":false,"hackerone_employee":false},"genius_execution_id":null,"team_handle":"yelp","actor_is_team_member":true,"actor_is_concealed_member":false}],"activity_page_count":1,"activity_page_number":1,"summaries":[{"id":24153,"category":"team","content":"@hk755a reported a clickjacking attack that allowed an attacker to overlay the checkout page (`/checkout/deal`), thereby potentially causing monetary loss for the victim.\n\nYelp was able to deploy an X-Frame-Options header in a short amount of time, and since then, Yelp has deployed a site-wide CSP policy to prevent such clickjacking attacks from occurring.","can_view?":true,"can_edit?":false,"content_html":"\u003cp\u003e\u003ca href=\"/hk755a\"\u003e@hk755a\u003c/a\u003e reported a clickjacking attack that allowed an attacker to overlay the checkout page (\u003ccode\u003e/checkout/deal\u003c/code\u003e), thereby potentially causing monetary loss for the victim.\u003c/p\u003e\n\n\u003cp\u003eYelp was able to deploy an X-Frame-Options header in a short amount of time, and since then, Yelp has deployed a site-wide CSP policy to prevent such clickjacking attacks from occurring.\u003c/p\u003e\n"},{"category":"researcher","can_view?":true,"can_create?":false}]}