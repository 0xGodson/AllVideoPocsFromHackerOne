ect_properties_init_ex(Z_OBJ_P(arg), properties);\n    } else {\n        object_properties_init(Z_OBJ_P(arg), class_type);\n    }\n} else {\n    ZVAL_OBJ(arg, class_type-\u0026gt;create_object(class_type));\n}\nreturn SUCCESS;\n\u003c/code\u003e\u003c/pre\u003e\u003c/div\u003e\u003cdiv class=\"highlight\"\u003e\u003cpre class=\"highlight plaintext\"\u003e\u003ccode\u003e\u003c/code\u003e\u003c/pre\u003e\u003c/div\u003e\n\u003cp\u003eobject_init_ex(\u0026amp;obj, pce);\u003c/p\u003e\n\u003cdiv class=\"highlight\"\u003e\u003cpre class=\"highlight plaintext\"\u003e\u003ccode\u003e                        /* Merge current hashtable with object\u0026#39;s default properties */\n                        zend_hash_merge(Z_OBJPROP(obj),\n                                        Z_ARRVAL(ent2-\u0026gt;data),\n                                        zval_add_ref, 0);\n\u003c/code\u003e\u003c/pre\u003e\u003c/div\u003e\u003cdiv class=\"highlight\"\u003e\u003cpre class=\"highlight plaintext\"\u003e\u003ccode\u003e\nTest script:\n---------------\n\u003c/code\u003e\u003c/pre\u003e\u003c/div\u003e\n\u003cp\u003e\u0026lt;?php\u003cbr\u003e\n$xml = \u0026lt;\u0026lt;\u0026lt;EOF\u003cbr\u003e\n\u0026lt;?xml version=\u0026quot;1.0\u0026quot; ?\u0026gt;\u003cbr\u003e\n\u0026lt;wddxPacket version=\u0026quot;1.0\u0026quot;\u0026gt;\u003cbr\u003e\n    \u0026lt;struct\u0026gt;\u003cbr\u003e\n        \u0026lt;var name=\u0026quot;php_class_name\u0026quot;\u0026gt;\u003cbr\u003e\n            \u0026lt;string\u0026gt;Throwable\u0026lt;/string\u0026gt;\u003cbr\u003e\n                \u0026lt;/var\u0026gt;\u003cbr\u003e\n        \u0026lt;/struct\u0026gt;\u003cbr\u003e\n\u0026lt;/wddxPacket\u0026gt;\u003cbr\u003e\nEOF;\u003cbr\u003e\n    $wddx = wddx_deserialize($xml);\u003cbr\u003e\n    var_dump($wddx);\u003cbr\u003e\n?\u0026gt;\u003c/p\u003e\n\u003cdiv class=\"highlight\"\u003e\u003cpre class=\"highlight plaintext\"\u003e\u003ccode\u003e\u003c/code\u003e\u003c/pre\u003e\u003c/div\u003e","bounty_amo